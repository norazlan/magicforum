<%= form_tag sessions_path  do |f| %>
<div class="form-group">
  <h4> <%= f.label :title_field, "Email" %> </h4>
  <%= f.text_field :email, required: true, id: "user_email_field", class: 'form-control' %>
</div>
<div class="form-group">
  <h4> <%= f.label :title_field, "Password" %> </h4>
  <%= f.text_field :password, required: true, id: "user_email_field", class: 'form-control' %>
</div>
<%= f.submit "Login", class: "btn btn-success btn-lg" %>
<% end %>
